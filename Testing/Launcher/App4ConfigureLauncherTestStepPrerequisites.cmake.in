
if(NOT DEFINED WITH_CONFIG_FILE)
  message(FATAL_ERROR "Make sure variable WITH_CONFIG_FILE is defined")
endif()

set(app4configurelaunchertest_source_dir "@APP4CONFIGURELAUNCHERTEST_SOURCE_DIR@")
if(WITH_CONFIG_FILE)
  set(app4configurelaunchertest_binary_dir "@APP4CONFIGURELAUNCHERTEST_WITH_CONFIG_FILE_BINARY_DIR@")
else()
  set(app4configurelaunchertest_binary_dir "@APP4CONFIGURELAUNCHERTEST_BINARY_DIR@")
endif()
set(app4configurelaunchertest_build_type "@APP4CONFIGURELAUNCHERTEST_BUILD_TYPE@")
set(app4configurelaunchertest_cmake_generator "@APP4CONFIGURELAUNCHERTEST_CMAKE_GENERATOR@")

set(ctkapplauncher_dir "@LAUNCHER_DIR@")

# --------------------------------------------------------------------------
